{
  "name": "hotellux-mysql",
  "version": "1.0.0",
  "description": "HotelLux - Hotel Management System with MySQL",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "NODE_ENV=development tsx server/index.ts",
    "client": "vite --host 0.0.0.0",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "setup": "npm install && npm run db:setup",
    "db:setup": "mysql -u root -p < database_export.sql"
  },
